{
    "name": "eicc/staticforge",
    "version": "1.18.4",
    "description": "Static site generator built in PHP with extensible feature system",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cal Evans",
            "email": "cal@calevans.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "eicc/staticforge-installer": ">=1.0",
        "eicc/utils": "^1.0",
        "gajus/dindent": "^2.0",
        "league/commonmark": "^2.4",
        "phpseclib/phpseclib": "^3.0",
        "symfony/console": "^6.0",
        "symfony/css-selector": "^8.0",
        "symfony/dom-crawler": "^8.0",
        "symfony/http-client": "^8.0",
        "symfony/yaml": "^6.0",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "calevans/staticforge-chapternav": "1.1.2",
        "calevans/staticforge-google-analytics": "^1.1",
        "calevans/staticforge-social-metadata": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5",
        "shipmonk/dead-code-detector": "^0.13.5",
        "squizlabs/php_codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "EICC\\StaticForge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EICC\\StaticForge\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "eicc/staticforge-installer": true
        }
    },
    "bin": [
        "bin/staticforge.php"
    ]
}

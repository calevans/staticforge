{
    "name": "eicc/staticforge",
    "description": "Static site generator built in PHP with extensible feature system",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Cal Evans",
            "email": "cal@calevans.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "vlucas/phpdotenv": "^5.5",
        "twig/twig": "^3.0",
        "symfony/console": "^6.0",
        "symfony/yaml": "^6.0",
        "league/commonmark": "^2.4",
        "eicc/utils": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "EICC\\StaticForge\\": "src/",
            "EICC\\Utils\\": "vendor/eicc/utils/src/Utils/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EICC\\StaticForge\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
